	},
	"peerDependencies": {
		"@nestjs/bullmq": "^11.0.0",
		"@nestjs/common": "^11.0.0",
		"@nestjs/config": "^4.0.0",
		"@nestjs/core": "^11.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.11.30",
		"@types/nodemailer": "^6.4.16",
		"typescript": "^5.6.3"
	},
	"engines": {
		"node": ">=18"
	}
